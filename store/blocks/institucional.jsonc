    "flex-layout.row#shelf-institucional-01":{
        "children":[
            "list-context.product-list#alfrdlist2"
        ]
    },

    "list-context.product-list#alfrdlist2": {
        "blocks": [
            "product-summary.shelf#alfrd-shelf1"
        ],
        "children": [
            "slider-layout#testprod2"
        ],
        "props": {
            "category": "4"
        }
      },

      "slider-layout#testprod2": {
        "props": {
          "itemsPerPage": {
            "desktop": 4,
            "tablet": 4,
            "phone": 4
          },
          "infinite": true,
          "showNavigationArrows": "desktopOnly",
          "blockClass": "carousel",
          "fullWidth": true
        }
      },

    "flex-layout.row#tab-layout":{
        "children":[
            "tab-layout#tabs"
        ]
    },

    "tab-layout#tabs":{
        "children":[
            "tab-list#tabs",
            "tab-content#tabs"
        ],
        "props":{
            "defaultActiveTabId": "tab1"
        }
    },

    "tab-list#tabs":{
        "children":[
            "tab-list.item#tab1",
            "tab-list.item#tab2"
        ]
    },

    "tab-list.item#tab1":{
        "props":{
            "tabId": "tab1",
            "label": "Item 01",
            "defaultActiveTab": true
        }
    },

    "tab-list.item#tab2":{
        "props":{
            "tabId": "tab2",
            "label": "Item 02"
        }
    },

    "tab-content#tabs":{
        "children":[
            "tab-content.item#content1",
            "tab-content.item#content2"
        ]
    },
    "tab-content.item#content1":{
        "children":[
            "image#1"
        ],
        "props":{
            "tabId": "tab1"
        }
    },
    "tab-content.item#content2":{
        "children":[
            "image#banner2"
        ],
        "props":{
            "tabId": "tab2"
        }
    }



}